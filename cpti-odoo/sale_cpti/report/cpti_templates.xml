<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!-- Added Cpti standard report format -->
<record id="cpti_standard_report_paper_format" model="report.paperformat">
    <field name="name">Cpti Standard Report Format</field>
    <field name="default" eval="True" />
    <field name="page_height">0</field>
    <field name="page_width">0</field>
    <field name="orientation">Portrait</field>
    <field name="margin_top">25</field>
    <field name="margin_bottom">20</field>
    <field name="margin_left">5</field>
    <field name="margin_right">5</field>
    <field name="header_line" eval="False" />
    <field name="header_spacing">12</field>
    <field name="dpi">90</field>
    <field name="report_ids" eval="[(4, ref('sale.action_report_saleorder'))]" />
</record>
</odoo>